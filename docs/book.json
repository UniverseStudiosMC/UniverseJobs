{
  "title": "JobsAdventure Wiki",
  "description": "Documentation complète pour JobsAdventure - Le plugin de métiers ultime pour Minecraft",
  "author": "Ax_Dev",
  "language": "fr",
  "root": "./",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "theme-comscore",
    "search",
    "lunr",
    "sharing",
    "fontsettings",
    "livereload",
    "copy-code-button",
    "expandable-chapters",
    "back-to-top-button",
    "page-toc",
    "github",
    "edit-link",
    "prism",
    "sitemap-general",
    "favicon"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true,
      "styles": {
        "website": "styles/website.css"
      }
    },
    "search": {
      "maxIndexSize": 1000000
    },
    "sharing": {
      "facebook": false,
      "twitter": false,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": []
    },
    "fontsettings": {
      "theme": "white",
      "family": "sans",
      "size": 2
    },
    "github": {
      "url": "https://github.com/ax-dev/JobsAdventure"
    },
    "edit-link": {
      "base": "https://github.com/ax-dev/JobsAdventure/tree/main/docs",
      "label": "Éditer cette page"
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    },
    "sitemap-general": {
      "prefix": "https://jobsadventure.wiki/"
    },
    "favicon": {
      "shortcut": "assets/favicon.ico",
      "bookmark": "assets/favicon.ico"
    }
  },
  "pdf": {
    "pageNumbers": true,
    "fontSize": 12,
    "fontFamily": "Arial",
    "paperSize": "a4",
    "chapterMark": "pagebreak",
    "pageBreaksBefore": "/",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 56,
      "bottom": 56
    }
  }
}